class Solution {
public:
    void rotate(vector<int>& nums, int k) {
      int n=nums.size(); 
        
        vector<int> temp(n); 
        
        for(int i=0;i<n;i++) 
        { 
            temp[(i+k)%nums.size()] = nums[i]; 
        } 
        nums = temp; 
    }
}; 

// time complexity : o(n) 
// space complexity : o(n) 
